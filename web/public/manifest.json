{
  "manifest_version": 3,
  "name": "Croissant 🥐",
  "version": "0.1",
  "action": {
    "default_title": "Croissant",
    "default_popup": "index.html"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval';"
  }
}
